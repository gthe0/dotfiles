{
    "workbench.iconTheme": "vscode-icons",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "json.format.keepLines": true,
    "terminal.integrated.profiles.windows": {

        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Windows PowerShell": {
            "path": "${env:windir}\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
        },
        "Developer PowerShell":
        {
            "source": "PowerShell",
            "icon": "terminal-powershell",
            "args": [
                "-NoLogo"
                ,"-NoExit"
                ,"-Command"
                ,"&{${env:VSINSTALLDIR}\\Common7\\Tools\\Microsoft.VisualStudio.DevShell.dll'; Enter-VsDevShell b9de59ab -SkipAutomaticLocation -DevCmdArguments '-arch=x64 -host_arch=x64'}"
            ],
        },
    },
    "terminal.integrated.defaultProfile.windows": "Developer PowerShell",
    "security.workspace.trust.banner": "never",
    "terminal.integrated.autoReplies": {},
    "cmake.configureOnOpen": true,
    "python.defaultInterpreterPath": "${env:USERPROFILE}\\anaconda3\\envs\\pdf_reader\\python.exe",
    "workbench.startupEditor": "none",
    "zenMode.hideLineNumbers": false,
    "zenMode.centerLayout": false,
    "zenMode.fullScreen": false,
    "zenMode.hideStatusBar": false,
    "telemetry.telemetryLevel": "off",
    "vscode_custom_css.imports": [
        "file:///%HOMEPATH%/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.1/dist/updateHandler.js"
    ],
    "git.confirmSync": false,
    "prettier.useTabs": true,
    "editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"editor.minimap.enabled": false,
	"diffEditor.ignoreTrimWhitespace": false,
	"cmake.pinnedCommands": [
		"workbench.action.tasks.configureTaskRunner",
		"workbench.action.tasks.runTask"
	],
	"json.schemas": [
	
	]
}